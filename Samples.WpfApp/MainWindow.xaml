<Window x:Class="Samples.WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Samples.WpfApp"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="WPF App (3rd party sample)" Height="250" Width="350">
    <Grid Margin="15" Button.Click="Button_Click">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        <Button AutomationProperties.AutomationId="01" Content="01" Grid.Row="0" Grid.Column="0" Margin="10"  />
        <Button AutomationProperties.AutomationId="02" Content="02" Grid.Row="0" Grid.Column="1" Margin="10" />
        <Button AutomationProperties.AutomationId="03" Content="03" Grid.Row="1" Grid.Column="0" Margin="10" />
        <Button AutomationProperties.AutomationId="04" Content="04" Grid.Row="1" Grid.Column="1" Margin="10" />

    </Grid>
</Window>
